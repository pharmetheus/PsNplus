$PROBLEM    MOXONIDINE PK,FINAL ESTIMATES,simulated data
$INPUT      ID VISI XAT2=DROP DGRP DOSE FLAG=DROP ONO=DROP XIME=DROP
            DVO=DROP NEUY SCR AGE=DROP SEX=DROP NYHA=DROP WT=DROP DROP
            ACE DIG DIU=DROP NUMB=DROP TAD TIME VIDD=DROP CRCL=DROP
            AMT SS II DROP CMT=DROP CONO=DROP DV EVID=DROP OVID=DROP
$DATA      ../../../mox_simulated.csv IGNORE=@
$SUBROUTINE ADVAN2 TRANS1
$OMEGA  BLOCK(2)
 0.415573
 0.390528 0.570966  ; IIV (CL-V)
$OMEGA  BLOCK(1)
 0.269524  ;     IIV KA
$OMEGA  BLOCK(1)
 1.47E-10  ;     IOV CL
$OMEGA  BLOCK(1) SAME
$OMEGA  BLOCK(1)
 5.06E-09  ;     IOV KA
$OMEGA  BLOCK(1) SAME
$PK

;;; CLDIG-DEFINITION START
IF(DIG.EQ.1.0000E+00) CLDIG = 1  ; Most common
IF(DIG.EQ.0.0000E+00) CLDIG = ( 1 + THETA(6))
;;; CLDIG-DEFINITION END

;;; CL-RELATION START
CLCOV=CLDIG
;;; CL-RELATION END


;;; VACE-DEFINITION START
IF(ACE.EQ.1.0000E+00) VACE = 1  ; Most common
IF(ACE.EQ.0.0000E+00) VACE = ( 1 + THETA(5))
;;; VACE-DEFINITION END

;;; V-RELATION START
VCOV=VACE
;;; V-RELATION END


;-----------OCCASIONS----------
   VIS3               = 0
   IF(VISI.EQ.3) VIS3 = 1
   VIS8               = 0
   IF(VISI.EQ.8) VIS8 = 1

;----------IOV--------------------
   
   KPCL  = VIS3*ETA(4)+VIS8*ETA(5)
   KPKA  = VIS3*ETA(6)+VIS8*ETA(7)

;---------- PK model ------------------

   TVCL  = THETA(1)

TVCL = CLCOV*TVCL
   TVV   = THETA(2)

TVV = VCOV*TVV

   CL    = TVCL*EXP(ETA(1)+KPCL)
   V     = TVV*EXP(ETA(2))
   KA    = THETA(3)*EXP(ETA(3)+KPKA)
   ALAG1 = THETA(4)
   K     = CL/V
   S2    = V

$ERROR


     IPRED = LOG(.025)
     WA     = 1
     W      = WA
     IF(F.GT.0) IPRED = LOG(F)
     IRES  = IPRED-DV
     IWRES = IRES/W
     Y     = IPRED+ERR(1)*W

$THETA  (0,32.8907) ; TVCL
$THETA  (0,124.292) ; TVV
$THETA  (0,1.45927) ; TVKA
$THETA  (0,0.0830626) ; LAG
$THETA  (-1,-0.187842,5) ; VACE1
$THETA  (-1,-0.001,5) ; CLDIG1
$SIGMA  0.112186
$ESTIMATION METHOD=1 MAXEVAL=9999


